{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Brimborium-MessageFlow-FrontEnd": {
      "projectType": "library",
      "root": "projects/brimborium-message-flow-front-end",
      "sourceRoot": "projects/brimborium-message-flow-front-end/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/brimborium-message-flow-front-end/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/brimborium-message-flow-front-end/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/brimborium-message-flow-front-end/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/brimborium-message-flow-front-end/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "00cb33b4-aa66-4bcf-8756-5be03aca415b"
  }
}
